{
  "version": 2,
  "builds": [
    {
      "src": "index.php",
      "use": "@vercel/php"
    },
    {
      "src": "app/**/*.php",
      "use": "@vercel/php"
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.php?url=$1"
    }
  ]
}