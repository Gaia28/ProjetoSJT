{
    "version": 2,
    "functions": {
    "api/*.php": {
      "runtime": "vercel-php@8.1.25"
    }
  },
    "routes": [
        {
            "src": "/public/(.*)",
            "dest": "/public/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ]
}